{
  "name": "react-tsx-project",
  "version": "1.0.0",
  "description": "Classic React project with TypeScript support",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development --open",
    "start:edge": "webpack serve --mode development --open --env browser=edge",
    "build": "webpack --mode production",
    "build:analyze": "webpack --mode production --analyze",
    "dev": "webpack serve --mode development",
    "dev:no-open": "webpack serve --mode development --no-open",
    "type-check": "tsc --noEmit",
    "deploy": "powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "deploy:win": "deploy.bat",
    "deploy:cloudflare": "npm run build && npx wrangler pages deploy dist",
    "preview": "npx serve dist"
  },
  "keywords": [
    "react",
    "typescript",
    "tsx",
    "frontend"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
